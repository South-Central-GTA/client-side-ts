{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es6",
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "inlineSourceMap": false,
    "declaration": false,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@altv"
    ],
    "rootDir": "./",
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowJs": true,
    "importHelpers": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@interfaces/*": [
        "interfaces/*"
      ],
      "@enums/*": [
        "enums/*"
      ],
      "@extensions/*": [
        "extensions/*"
      ]
    }
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}